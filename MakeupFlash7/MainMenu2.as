package  {		import flash.display.MovieClip;	import flash.display.SimpleButton;	import flash.events.MouseEvent;	import flash.net.navigateToURL;	import flash.net.URLRequest;	import flash.net.URLLoader;	import flash.media.Sound;    import flash.media.SoundChannel;	import flash.media.SoundTransform;		public class MainMenu2 extends MovieClip	{		private var donMakeupBtn_mc:MovieClip;		private var HomeBtn_mc:MovieClip;		private var video_mc:MovieClip;		private var soundBtn_mc:MovieClip;		private var logoBtn_mc:SimpleButton;				private var snd:Sound = new Sound();        private var channel:SoundChannel = new SoundChannel();		private var songTransform:SoundTransform;		public function MainMenu2() 		{			var req:URLRequest = new URLRequest("jiajiang.mp3");            snd.load(req);						channel = snd.play(0,10000);						songTransform = channel.soundTransform;			songTransform.volume = 0.3;			channel.soundTransform = songTransform;						HomeBtn_mc = new HomeBtn();			addChild(HomeBtn_mc);			HomeBtn_mc.x=720;			HomeBtn_mc.y=10;			HomeBtn_mc.btn.visible=true;			HomeBtn_mc.homeMarker.visible=false;						video_mc = new JiaJiangBtn();			addChild(video_mc);			video_mc.x=787;			video_mc.y=10;			video_mc.btn.visible=true;			video_mc.jiajiangMarker.visible=false;						donMakeupBtn_mc = new donMakeupBtn();			addChild(donMakeupBtn_mc);			donMakeupBtn_mc.x=861;			donMakeupBtn_mc.y=10;			donMakeupBtn_mc.btn.visible=false;			donMakeupBtn_mc.markerDon.visible=true;						soundBtn_mc = new soundBtn();			addChild(soundBtn_mc);			soundBtn_mc.x=1045;			soundBtn_mc.y=10;			soundBtn_mc.markerOn.visible=false;			soundBtn_mc.markerOff.visible=false;			soundBtn_mc.onToOff.visible=true;			soundBtn_mc.offToOn.visible=false;						logoBtn_mc = new logoBtn();			addChild(logoBtn_mc);			logoBtn_mc.x=525;			logoBtn_mc.y=15;						logoBtn_mc.addEventListener(MouseEvent.CLICK, logoHander);			HomeBtn_mc.addEventListener(MouseEvent.CLICK, homeHander);			donMakeupBtn_mc.addEventListener(MouseEvent.CLICK, donMakeupHander);			video_mc.addEventListener(MouseEvent.CLICK, videoHander);			soundBtn_mc.onToOff.addEventListener(MouseEvent.CLICK, soundstopHander);			soundBtn_mc.offToOn.addEventListener(MouseEvent.CLICK, soundplayHander);		}				private function logoHander(e:MouseEvent)		{			var req:URLRequest=new URLRequest("http://sunny.chunhuanglo.com/2012JiaJIang/JiaJiangInfo/prloader2.html"); 	    	navigateToURL(req,"_self"); 		}				private function homeHander(e:MouseEvent)		{			var req:URLRequest=new URLRequest("http://sunny.chunhuanglo.com/2012JiaJIang/JiaJiangInfo/prloader2.html"); 	    	navigateToURL(req,"_self"); 		}				private function videoHander(e:MouseEvent)		{			var req:URLRequest=new URLRequest("http://sunny.chunhuanglo.com/movie/test2_2.html"); 	    	navigateToURL(req,"_self"); 		}						private function donMakeupHander(e:MouseEvent)		{			var req:URLRequest=new URLRequest("http://sunny.chunhuanglo.com/2012JiaJIang/makeupTestOld/testFB.html"); 	    	navigateToURL(req,"_self");		}				private function soundstopHander(e:MouseEvent)		{			channel.stop();			soundBtn_mc.onToOff.visible=false;			soundBtn_mc.offToOn.visible=true;		}				private function soundplayHander(e:MouseEvent)		{			channel = snd.play(0,10000);			soundBtn_mc.onToOff.visible=true;			soundBtn_mc.offToOn.visible=false;		}	}	}